{
    "login": {
        "header": "T",
        "format": [
            {"key": "serial", "bytes": 0, "type": "string"}
        ]
    },
    "report": {
        "header": "P",
        "format": [
            {"key": "positions", "bytes": 19, "type": "array", "array": [
                {"key": "time", "bytes": 6, "type": "datetime"},
                {"key": "lng", "bytes": 4, "type": "float", "decimals": 6},
                {"key": "lat", "bytes": 4, "type": "float", "decimals": 6},
                {"key": "speed", "bytes": 1, "type": "unsigned"},
                {"key": "mark", "bytes": 2, "type": "unsigned"},
                {"key": "busstop", "bytes": 2, "type": "unsigned"}
            ]},
            {"key": "trip", "bytes": 4, "type": "unsigned"},
            {"key": "route", "bytes": 1, "type": "unsigned"},
            {"key": "direction", "bytes": 1, "type": "bool"},
            {"key": "state", "bytes": 1, "type": "char"},
            {"key": "sales", "bytes": 3, "type": "unsigned"},
            {"key": "events", "bytes": 3, "type": "array", "array": [
                {"key": "id", "bytes": 2, "type": "unsigned"},
                {"key": "value", "bytes": 1, "type": "unsigned"}
            ]}
        ]
    }
}
